<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Pagination?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="anchor" prefHeight="513.0" prefWidth="673.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Afficher_EnfantController">
   <children>
      <ImageView fitHeight="538.0" fitWidth="774.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Assets/Interface_sport.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="tab_enf" layoutX="40.0" layoutY="73.0" onMouseClicked="#liste_enf" prefHeight="399.0" prefWidth="552.0">
        <columns>
          <TableColumn id="id_enfant" fx:id="aff_ide" prefWidth="75.0" text="Id_Enfant" />
          <TableColumn id="nom" fx:id="aff_nome" prefWidth="75.0" text="Nom" />
            <TableColumn id="prenom" fx:id="aff_prenom" prefWidth="75.0" text="Prenom" />
            <TableColumn id="age" fx:id="aff_age" prefWidth="75.0" text="Age" />
            <TableColumn id="sexe" fx:id="aff_sexe" prefWidth="75.0" text="Sexe" />
            <TableColumn id="photo" fx:id="aff_photo" prefWidth="75.0" text="Photo" />
            <TableColumn id="idu" fx:id="aff_idu" prefWidth="75.0" text="ID_User" />
        </columns>
      </TableView>
      <Button fx:id="del_enf" layoutX="662.0" layoutY="191.0" mnemonicParsing="false" onAction="#supprimer_enfant" text="Supprimer" />
      <Button fx:id="upd_enf" layoutX="672.0" layoutY="258.0" mnemonicParsing="false" onAction="#modifier_enfant" text="Modifier" />
      <Pagination fx:id="pagination_E" layoutX="165.0" layoutY="8.0" prefHeight="65.0" prefWidth="248.0" />
      <Button fx:id="Excel" layoutX="672.0" layoutY="328.0" mnemonicParsing="false" onAction="#Excel" text="Excel" />
      <Button fx:id="button" layoutX="668.0" layoutY="389.0" mnemonicParsing="false" onAction="#handleButtonAction" text="Go Page" />
   </children>
</AnchorPane>
