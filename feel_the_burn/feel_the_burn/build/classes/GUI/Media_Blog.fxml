<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane id="AnchorPane" prefHeight="456.0" prefWidth="643.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Media_BlogController">
   <children>
      <MediaView fx:id="mediaview" fitHeight="400.0" fitWidth="400.0" layoutX="120.0" layoutY="28.0" />
      <Slider fx:id="volume" layoutX="223.0" layoutY="428.0" onDragDetected="#click" />
      <Button fx:id="play" layoutX="569.0" layoutY="151.0" mnemonicParsing="false" onAction="#play" text="play" />
      <Button fx:id="pause" layoutX="562.0" layoutY="203.0" mnemonicParsing="false" onAction="#pause" text="pause" />
      <Button fx:id="stop" layoutX="562.0" layoutY="259.0" mnemonicParsing="false" onAction="#stop" text="stop" />
   </children>
</AnchorPane>
